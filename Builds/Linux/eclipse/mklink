if [ ! -d Utility ]
then 
mkdir Utility
fi
if [ -d ./Utility/src ]
then
    if [ -d ./Utility/src/Utility ]
    then
        rm ./Utility/src/Utility
    fi
    rm ./Utility/src
fi
ln -s ./../../../../Source/Common/Utility/ ./Utility/src

if [ ! -d Runtime ]
then
mkdir Runtime
fi
if [ -d ./Runtime/src ]
then
    if [ -d ./Runtime/src/Runtime ]
    then
        rm ./Runtime/src/Runtime
    fi
    rm ./Runtime/src
fi

ln -s ./../../../../Source/Common/Runtime/ ./Runtime/src

